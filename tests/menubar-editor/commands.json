{
  "activate menubar": {
    "reading": {
      "jaws": [ [ "DOWN" ], [ "ENTER" ] ],
      "nvda": [ [ "SPACE" ], [ "ENTER" ] ]
    }
  },
  "tab to menubar": {
    "reading": {
      "jaws": [ [ "TAB_AND_SHIFT_TAB" ] ],
      "nvda": [ [ "TAB_AND_SHIFT_TAB" ] ]
    }
  },
  "navigate to menubar": {
    "reading": {
      "jaws": [
        [ "UP_AND_DOWN" ],
        [ "F_AND_SHIFT_F" ]
      ],
      "nvda": [
        [ "TAB_AND_SHIFT_TAB" ],
        [ "UP_AND_DOWN" ],
        [ "INS_DOWN_OR_CAPS_DOWN" ],
        [ "NUMPAD_PLUS" ]
      ]
    }
  },
  "navigate to menuitem in menubar": {
    "reading": {
      "jaws": [ [ "RIGHT" ] ],
      "nvda": [ [ "RIGHT" ] ]
    },
    "interaction": {
      "jaws": [ [ "RIGHT" ] ],
      "nvda": [ [ "RIGHT" ] ],
      "voiceover_macos": [
        [
          "CTRL_OPT_RIGHT_AND_CTRL_OPT_LEFT"
        ],
        [
          "CTRL_OPT_A",
          "(Start reading)"
        ],
        [ "LEFT_AND_RIGHT" ]
      ]
    }
  },
  "navigate to menuitemradio in submenu": {
    "interaction": {
      "jaws": [ [ "UP_AND_DOWN" ] ],
      "nvda": [ [ "DOWN" ] ]
    }
  },
  "navigate to menuitemcheckbox in submenu": {
    "interaction": {
      "jaws": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "DOWN" ]
      ],
      "nvda": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "DOWN" ]
      ],
      "voiceover_macos": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "DOWN" ]
      ]
    }
  },
  "change state of menuitemradio in submenu to checked": {
    "interaction": {
      "jaws": [ [ "ENTER" ], [ "SPACE" ] ],
      "nvda": [ [ "ENTER" ], [ "SPACE" ] ],
      "voiceover_macos": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "CTRL_OPT_SPACE" ]
      ]
    }
  },
  "change state of menuitemcheckbox in submenu to checked": {
    "interaction": {
      "jaws": [ [ "ENTER" ], [ "SPACE" ] ],
      "nvda": [ [ "ENTER" ], [ "SPACE" ] ],
      "voiceover_macos": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "CTRL_OPT_SPACE" ]
      ]
    }
  },
  "change state of menuitemcheckbox in submenu to unchecked": {
    "interaction": {
      "jaws": [ [ "ENTER" ], [ "SPACE" ] ],
      "nvda": [ [ "ENTER" ], [ "SPACE" ] ],
      "voiceover_macos": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "CTRL_OPT_SPACE" ]
      ]
    }
  },
  "open submenu of menubar": {
    "reading": {
      "jaws": [ [ "ENTER" ], [ "SPACE" ] ],
      "nvda": [ [ "ENTER" ], [ "SPACE" ] ]
    },
    "interaction": {
      "jaws": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "UP_AND_DOWN" ]
      ],
      "nvda": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "DOWN" ]
      ],
      "voiceover_macos": [
        [ "ENTER" ],
        [ "SPACE" ],
        [ "DOWN" ]
      ]
    }
  },
  "close submenu of menubar": {
    "interaction": {
      "jaws": [ [ "ESC" ] ],
      "nvda": [ [ "ESC" ] ],
      "voiceover_macos": [ [ "ESC" ] ]
    }
  },
  "read menuitem in menubar": {
    "reading": {
      "jaws": [ [ "INS_TAB" ], [ "INS_UP" ] ],
      "nvda": [
        [ "INS_DOWN_OR_CAPS_DOWN" ],
        [ "NUMPAD_5" ],
        [ "NUMPAD_PLUS" ]
      ]
    },
    "interaction": {
      "jaws": [ [ "INS_TAB" ], [ "INS_UP" ] ],
      "nvda": [
        [ "INS_DOWN_OR_CAPS_DOWN" ],
        [ "NUMPAD_5" ],
        [ "NUMPAD_PLUS" ]
      ],
      "voiceover_macos": [ [ "CTRL_OPT_A" ] ]
    }
  },
  "read menuitemradio in a group in a submenu": {
    "interaction": {
      "jaws": [ [ "INS_TAB" ], [ "INS_UP" ] ],
      "nvda": [
        [ "INS_DOWN_OR_CAPS_DOWN" ],
        [ "NUMPAD_5" ],
        [ "NUMPAD_PLUS" ]
      ]
    },
    "item": {
      "voiceover_macos": [ [ "INS_TAB" ], [ "INS_UP" ] ]
    }
  },
  "read menuitemcheckbox in a submenu": {
    "interaction": {
      "jaws": [ [ "INS_TAB" ], [ "INS_UP" ] ],
      "nvda": [
        [ "INS_DOWN_OR_CAPS_DOWN" ],
        [ "NUMPAD_5" ],
        [ "NUMPAD_PLUS" ]
      ],
      "voiceover_macos": [ [ "CTRL_OPT_A" ] ]
    }
  },
  "read disabled menuitem in a submenu": {
    "interaction": {
      "jaws": [ [ "INS_TAB" ], [ "INS_UP" ] ],
      "nvda": [ [ "INS_NUMPAD_5" ] ],
      "voiceover_macos": [ [ "CTRL_OPT_A" ] ]
    }
  }
}